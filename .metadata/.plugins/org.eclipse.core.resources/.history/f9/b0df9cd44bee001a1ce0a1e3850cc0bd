<script>
	function deletekeyword(id){
		fetch("/craw/keyword/delete/" +id,{
			method:"delete"
		}).then(function(resp){
			return resp.text();
		}).then(function(resp){
			alert("키워드 삭제 성공");
			location.reload();
		});
	}
</script>
